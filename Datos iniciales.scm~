(newline)
(define parametros read-line)
	(cond
  		(string? parametros
  			(string-split "parametros")
  			(string-split " ")
  			(string-split #:trim? #f)
  			 (string-locale=? parametros)
)
                )
(display "Error! Expresion no valida")
(display " saliendo ...." )
(display "gracias por usar nuestro codificador ")
